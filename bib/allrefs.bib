	journal = {J Orthop Surg Res},
	keywords = {Activities of Daily Living Aged *Arthroplasty, Replacement, Hip Clinical Protocols Controlled Before-After Studies Female Humans *Length of Stay Male *Patient-Centered Care Quality of Life},
	number = {1},
